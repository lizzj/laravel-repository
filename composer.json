{
  "name": "morisawa_kana/laravel-repository",
  "description": "Laravel - Repositories to  the database layer",
  "keywords": [
    "laravel",
    "repository",
    "eloquent",
    "model",
    "cache"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "morisawa_kana"
    }
  ],
  "homepage": "http://morisawa_kana.github.io/laravel-repository",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "doctrine/orm": "^2.5",
    "illuminate/contracts": "~5.0",
    "mockery/mockery": "^1.3",
    "pagerfanta/pagerfanta": "~1.0.0",
    "phpstan/phpstan": "^1.4",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "~3.4",
    "vimeo/psalm": "^4.22",
    "zendframework/zend-paginator": "~2.3"
},
  "autoload": {
    "psr-4": {
      "Morisawa\\Repository\\": "src/Morisawa/Repository/",
      "Morisawa\\Fractal\\": "src/Morisawa/Fractal/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Morisawa\\Repository\\Providers\\RepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
